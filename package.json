{
  "main": "gulpfile.js",
  "dependencies": {
    "bootstrap": "^4.0.0",
    "font-awesome": "^4.7.0",
    "owl.carousel": "^2.2.0",
    "particles.js": "^2.0.0",
    "popper.js": "^1.12.3",
    "stackblur-canvas": "^1.4.0",
    "tooltip.js": "^1.1.5",
    "typed.js": "^2.0.5"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-css-cli": "^4.1.11",
    "config": "^1.30.0",
    "css-minify": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.7.0",
    "gulp-cli": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-count": "^1.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-if": "^2.0.2",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.0",
    "merge-stream": "^1.0.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^5.0.0",
    "postcss-flexbugs-fixes": "^3.1.0",
    "shelljs": "^0.8.1",
    "stylelint": "^9.1.1",
    "stylelint-config-recommended-scss": "^3.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^2.4.0",
    "uglify-js": "^3.0.27"
  },
  "scripts": {
    "css": "npm-run-all --parallel css-lint css-compile --sequential css-prefix css-minify*",
    "css-compile": "node build/build.js css-compile",
    "css-lint": "stylelint --syntax scss \"scss/**/*.scss\"",
    "css-minify": "node build/build.js css-minify",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"css/*.css\" \"!css/*.min.css\"",
    "js": "npm-run-all js-lint* js-compile js-minify",
    "js-compile": "npm-run-all --parallel js-compile-*",
    "js-minify": "npm-run-all --parallel js-minify-*",
    "dist": "npm-run-all --parallel css js",
    "test": "npm-run-all dist js-test docs-compile docs-lint bundlesize",
    "watch": "npm-run-all --parallel watch-css watch-js",
    "watch-css": "nodemon --ignore js/ --ignore dist/ -e scss -x \"npm run css\"",
    "watch-js": "nodemon --ignore scss/ --ignore js/dist/ --ignore dist/ -e js -x \"npm run js-compile\""
  },
  "license": "MIT"
}
